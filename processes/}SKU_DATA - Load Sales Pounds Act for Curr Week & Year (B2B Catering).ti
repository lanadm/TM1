    IF( vLskus @= 'BONELESS-HAM-HALF',
        cellgetn( 'SKU_DATA', vYear, vCategory, vLocations, '100116', vWeek, 'Ham Price per lb', '100' ),

    # TURKEY BREAST
    #---------------------------
