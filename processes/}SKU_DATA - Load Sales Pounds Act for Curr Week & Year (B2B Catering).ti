        cellgetn( 'SKU_DATA', vYear, vCategory, vLocations, '110103', vWeek, 'Ham Price per lb', '100' ),

    # ELSE
    #---------------------------
    0))))));

vSalesPounds = vGrossValue \ vPriceValue;

LoadingSku =
