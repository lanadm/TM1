    IF( vLskus @= 'WHOLE HAM', '101000',
    IF( vLskus @= 'QUARTER HAM', '100114',
    IF( vLskus @= 'BONELESS-HAM-WHOLE', '100115',
    IF( vLskus @= 'BONELESS-HAM-HALF', '100116',
    IF( vLskus @= 'TURKEY BREAST', '110103',
    '0'))))));

LoadingChannel = '400';
LoadingStat = 'Sales Pounds';

