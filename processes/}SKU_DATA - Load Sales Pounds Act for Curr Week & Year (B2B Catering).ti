        cellgetn( 'SKU_DATA', vYear, vCategory, vLocations, '101200', vWeek, 'Ham Price per lb', '100' ),

    # WHOLE HAM
    #---------------------------
    IF( vLskus @= 'WHOLE HAM',
        cellgetn( 'SKU_DATA', vYear, vCategory, vLocations, '101000', vWeek, 'Ham Price per lb', '100' ),

    # QUARTER HAM
