Location = CELLGETS( 't-strctr', TRIM( STR( NUMBR( subst( vLocationID, 1, 4 )), 4, 0 )), 'Name' );

FiscalWeek = SubsetGetElementName( 'Week', 'Current Week', 1 );

FiscalYear = 'FY ' | subst( cellgets( 'bpmControl', 'Current Year', 'Value' ), 3, 2 );

Category = 'Actual';

Lskus = 'SMG';

Channel = '100';

HamEvalScore =  vScore / 100;

HamEval = HamEvalScore * 5;


CellPutN( HamEvalScore, 'SKU_DATA', FiscalYear, Category, Location, Lskus, FiscalWeek, 'Ham Eval %', Channel );
CellPutN( HamEval, 'SKU_DATA', FiscalYear, Category, Location, Lskus, FiscalWeek, 'Ham Evaluation', Channel );

